{
    "name": "merzdan/swagger-assertions",
    "description": "Test your API requests and responses against your swagger definition",
    "keywords": [
        "api",
        "assertion",
        "phpunit",
        "swagger",
        "test",
        "testing"
    ],
    "license": "BSD-2-Clause",
    "homepage": "https://github.com/merzdan/SwaggerAssertions",
    "require": {
        "php": ">= 5.5.0",
        "phpunit/phpunit": "4 - 5",
        "rize/uri-template": "^0.3.0",
        "zendframework/zend-http": "2.5 - 3"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "~5.0",
        "psr/http-message": "^1.0",
        "symfony/http-foundation": "^2.7"
    },
    "suggest": {
        "guzzlehttp/guzzle": "For Guzzle constraints",
        "symfony/http-foundation": "For Symfony constraints"
    },
    "autoload": {
        "psr-4": {
            "SwaggerAssertions\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SwaggerAssertionsTest\\": "tests/"
        }
    }
}
